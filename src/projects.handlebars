<section class="projects">
    <div class="section-title center no-line">Projects <div class="standalone-line-center"></div></div>
    <p>Some of my work</p>
    <div id="number">1/4</div>
    <div class="container projects-container">
        {{#projects}}
        <div class="row project active">
            <div class="w-md-5 project-info align-center">
                <div class="project-title">{{title}}</div>
                <div class="project-description">
                    {{description}}
                </div>
                {{#if contributors}}
                <div class="contributors">
                    Contributors:
                    {{#each contributors as |item|}}
                        {{item.[0]}}&nbsp;
                    {{/each}}
                </div>

                {{/if}}
                <div class="project-technologies">
                    {{#technologies}}
                        <ul class="technologies frontend">
                            {{#frontend}}
                                <li class="{{this}}" title="{{this}}"></li>
                            {{/frontend}}
                        </ul>

                        <ul class="technologies backend">
                            {{#backend}}
                                <li class="{{this}}" title="{{this}}"></li>
                            {{/backend}}
                        </ul>
                    {{/technologies}}
                </div>
            </div>
            <div class="w-md-7">
                <div class="project-image align-center">
                    <div class="project-image-container">
                        <div class="switcher">
                            {{#images}}
                                <div class="project-image-item" style="background: url('{{this}}');"></div>
                            {{/images}}
                        </div>

                        <div class="project-image-overlay">
                            <a href="https://github.com/alexcambose/{{repoName}}" target="_blank" class="button button-github">{{>svg/github}}</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{/projects}}
        <h1>Other projects</h1>
        <div class="row" id="other-projects">
        </div>
    </div>
    <small>And many other top secret projects!</small>
</section>